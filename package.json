  "private": true,
  "bugs": {
    "url": "https://github.com/tylermercer/sleep-prompter/issues"
  },
  "homepage": "https://github.com/tylermercer/sleep-prompter#readme",
  "dependencies": {
    "webextension-polyfill": "^0.6.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-web-extension": "^1.5.2",
    "typescript": "^3.7.4",
    "web-ext": "^4.0.0",
    "web-ext-types": "^3.2.1"
  }
}
