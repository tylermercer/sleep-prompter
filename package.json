{
  "name": "sleep-prompter",
  "version": "0.1.0",
  "description": "A WebExtension that subtly prompts the user to think about sleep near their bedtime",
  "scripts": {
    "start": "run-p start:parcel start:webext",
    "start:parcel": "parcel watch --no-hmr src/manifest.json",
    "start:webext": "web-ext run",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tylermercer/sleep-prompter.git"
  },
  "author": "Dan & Tyler Mercer",
